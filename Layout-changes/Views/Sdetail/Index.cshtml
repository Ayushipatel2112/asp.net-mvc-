@model List<Layout_changes.Models.Student>

	<h1>Student Detail</h1>
	<table border="1">
	<tr>
		<th>Student ID</th>
		<th>Student Name</th>
		<th>Student Class</th>
		<th>Student Address</th>
	</tr>
	@foreach (var item in Model)
	{
		<tr>
			<td>@item.Id</td>
			<td>@item.Name</td>
			<td>@item.Class</td>
			<td>@item.Address</td>
		</tr>
	}
	</table>